{
  "logger": {
    "internalLogHours": 0.5
  },
  "processes": [
    {
      "id": "cluster-example",
      "cmd": "deno run -A app.ts",
      "cluster": {
        "instances": 3,
        "commonPort": 3456,
        "startPort": 4000,
        "strategy": "ip-hash"
      }
    },
    {
      "id": "periodic-example-task",
      "cmd": "deno run --allow-read ../basic/task.js",
      "cron": "*/20 * * * * *",
      "restartDelayMs": 3000
    }
  ],
  "plugins": [
    {
      "url": "../../plugins/web-interface/mod.ts",
      "options": {
        "port": 5000
      }
    }
  ]
}
